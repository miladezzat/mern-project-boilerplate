{
  "name": "boilerplate-mern",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "npm install --prefix front-end",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix front-end",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix front-end && npm build --prefix fron-end"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.1.0",
    "config": "^3.2.5",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.11",
    "morgan": "~1.9.1"
  },
  "description": "This project is boilerplate for mern stack auth system",
  "main": "app.js",
  "keywords": [
    "MERN",
    "auth",
    "login",
    "register",
    "mongodb",
    "react",
    "reactjs",
    "express",
    "expressjs",
    "nodejs"
  ],
  "author": "Milad E. Fahmy",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^2.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miladezzat/mern-project-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/miladezzat/mern-project-boilerplate/issues"
  },
  "homepage": "https://github.com/miladezzat/mern-project-boilerplate#readme"
}
